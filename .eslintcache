sName=\"d-flex align-items-center\">\r\n                                                <div className={\"chat-user-img me-3 ms-0\"}>\r\n                                                    {\r\n                                                        group.photoURL ? <img src={group.photoURL}\r\n                                                                              className=\"rounded-circle avatar-sm\"\r\n                                                                              alt=\"ÐÐ²Ð°Ñ‚Ð°Ñ€\"/> :\r\n                                                          <div className=\"avatar-sm\">\r\n                                                            <span\r\n                                                              className=\"avatar-title rounded-circle bg-soft-primary text-primary\">\r\n                                                                {group.displayName.charAt(0)}\r\n                                                            </span>\r\n                                                          </div>\r\n                                                    }\r\n                                                    {\r\n                                                        group.status && <span className={\"user-status\" + \" \" + group.status}></span>\r\n                                                    }\r\n                                                </div>\r\n                                                <div className=\"flex-1 overflow-hidden\">\r\n                                                    <h5 className=\"text-truncate font-size-14 mb-0\">\r\n                                                        {group.displayName}\r\n                                                        {\r\n                                                            group.unRead !== 0\r\n                                                                ? <Badge color=\"none\" pill className=\"badge-soft-danger float-end\">\r\n                